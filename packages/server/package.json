{
  "name": "server",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "tsc",
    "start": "dotenvx run -- node build/main.js",
    "start:dev": "dotenvx run -f ../../.env -- yarn nodemon src/main.ts | pino-pretty",
    "lint": "eslint",
    "lintfix": "eslint --fix",
    "test": "dotenvx run -f ./.env.test -- jest"
  },
  "dependencies": {
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-smithy/server-common": "^1.0.0-alpha.10",
    "@dotenvx/dotenvx": "^1.26.2",
    "@running/server": "workspace:^",
    "@smithy/types": "^2.12.0",
    "@types/node": "^20.11.27",
    "@types/pg": "^8.11.2",
    "axios": "^1.7.9",
    "axios-case-converter": "^1.1.1",
    "find-yarn-workspace-root": "^2.0.0",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "strava": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^10.3.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
